<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >

<mapper namespace="com.example.shop.admin.mapper.AdminMapper">
    <!-- 데이터 생성-->
    <insert id="insertProduct"  parameterType="ProductCreateRequest">

        insert into product(product_name,quantity,price,image,description,created_at ,updated_at) values (#{productName},#{quantity},#{price},#{image},#{description},Now(),NOW())

    </insert>




</mapper>